<div class="frame board-8843f1f0b166" @onclick="OnClickCallback">
    <div class="shape rect Block" style="background: @ImageString; background-size:64px; width: 64px; height: 64px;"></div>
    <p style="width:inherit; overflow:clip; text-align:center;">@BlockName</p>
</div>

@code {
    [Parameter]
    public string BlockName { get; set; } = "undefined";

    [Parameter]
    public string ImageString { get; set; } = "url(data:image/png;base64,)";
    [Parameter]
    public EventCallback<MouseEventArgs> OnClickCallback { get; set; }
}
